            "description": "passports",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "passports": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "activity_score": {
                            "type": "number",
                            "nullable": false,
                            "description": "The score activity credentials contribute to the passport score"
                          },
                          "identity_score": {
