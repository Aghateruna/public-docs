                                "minimum_builder_score_flag": {
                                  "type": "boolean",
                                  "nullable": false,
                                  "description": "Whether the builder score value has a minimum as a condition for claiming"
                                },
                                "minimum_builder_score": {
                                  "type": "number",
                                  "nullable": true,
                                  "description": "The minimum builder score that should be used if the minimum_builder_score_flag is true"
                                },
                                "owning_credential_flag": {
                                  "type": "boolean",
                                  "nullable": false,
                                  "description": "Whether the build should own a credential for the perk to be claimed"
                                },
                                "owning_credential_type": {
                                  "type": "string",
                                  "nullable": true,
                                  "description": "The type of the credential that builder needs to own if the owning_credential_flag is true"
                                },
                                "owning_credential_name": {
                                  "type": "string",
                                  "nullable": true,
                                  "description": "The name of the credential that builder needs to own if the owning_credential_flag is true"
                                }
                              }
                            },
                            "code": {
                              "type": "array",
                              "items": {
