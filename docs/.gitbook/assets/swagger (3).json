                            "nullable": true,
                            "description": "The last time the passport score was calculated"
                          },
                          "main_wallet": {
                            "type": "string",
                            "nullable": true,
                            "description": "The main wallet of the passport"
                          },
                          "passport_id": {
                            "type": "integer",
                            "nullable": true,
                            "description": "The id of the passport"
                          },
                          "score": {
                            "type": "number",
                            "nullable": true,
                            "description": "The score of the passport"
                          },
                          "passport_profile": {
                            "type": "object",
                            "nullable": false,
                            "properties": {
                              "bio": {
                                "type": "string",
                                "nullable": true,
                                "description": "The bio of the passport"
                              },
                              "display_name": {
                                "type": "string",
                                "nullable": true,
                                "description": "The display name of the passport"
                              },
                              "location": {
                                "type": "string",
                                "nullable": true,
                                "description": "The location of the passport"
                              },
                              "tags": {
                                "type": "array",
                                "items": {
                                  "type": "string",
                                  "description": "A tag describing the passport profile"
                                }
                              }
                            }
                          },
                          "verified_wallets": {
                            "type": "array",
                            "items": {
                              "type": "string",
                              "description": "The connected wallets of the passport"
                            }
                          }
                        }
                      }
                    },
                    "pagination": {
                      "type": "object",
                      "properties": {
                        "current_page": {
                          "type": "integer",
                          "description": "The current page of the fetched records"
                        },
                        "last_page": {
                          "type": "integer",
                          "description": "The last page of records"
                        },
                        "total": {
                          "type": "integer",
                          "description": "The total amount of records"
                        }
                      }
                    }
                  }
                }
              }
            }
          },
