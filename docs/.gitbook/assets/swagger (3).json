{
  "openapi": "3.0.1",
  "info": {
    "title": "API V1",
    "version": "v1"
  },
  "paths": {
    "/passport_ai/{passport_id}": {
      "get": {
        "summary": "Retrieves the Passport AI of another Passport",
        "tags": [
          "Passport AI"
        ],
        "parameters": [
          {
            "name": "passport_id",
            "in": "path",
            "description": "Passport id",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "X-API-KEY",
            "in": "header",
            "description": "Your Talent Protocol API key",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "AUTHORIZATION",
            "in": "header",
            "description": "Your auth token",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "AI information of a passport retrieved",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "passport_ai": {
                      "type": "object",
                      "properties": {
                        "ai_bio": {
                          "type": "string",
                          "nullable": true,
                          "description": "The AI-generated bio of the passport"
                        },
                        "ai_generated_at": {
                          "type": "string",
                          "format": "date-time",
                          "nullable": true,
                          "description": "The timestamp of AI bio generation"
                        },
                        "generating_ai": {
                          "type": "boolean",
                          "nullable": false,
                          "description": "Flag indicating if the AI generation is in progress"
                        },
                        "ai_tags": {
                          "type": "array",
                          "items": {
                            "type": "string",
                            "description": "An array of AI-generated tags describing the passport profile"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "unauthorized request"
          }
        }
      }
    },
    "/passport_ai/current": {
      "get": {
        "summary": "Retrieves the AI generated information of the current passport",
        "tags": [
          "Passport AI"
        ],
        "parameters": [
          {
            "name": "X-API-KEY",
            "in": "header",
            "description": "Your Talent Protocol API key",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "AUTHORIZATION",
            "in": "header",
            "description": "Your auth token",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "AI information of the current passport retrieved"
          },
          "401": {
            "description": "unauthorized request"
          }
        }
      }
    },
    "/passport_ai/{passport_id}/generate": {
      "post": {
        "summary": "Generates AI bio for another passport if criteria are met and current user admin",
        "tags": [
          "Passport AI"
        ],
        "parameters": [
          {
            "name": "passport_id",
            "in": "path",
            "description": "Passport id",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "X-API-KEY",
            "in": "header",
            "description": "Your Talent Protocol API key",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "AUTHORIZATION",
            "in": "header",
            "description": "Your auth token",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "AI information of a passport retrieved"
          },
